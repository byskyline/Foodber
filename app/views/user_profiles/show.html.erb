<h1><%= @profile.name %></h1>
<h2><%= @profile.description %></h2>

<% if current_user && @profile.user == current_user %>

<ul class="nav nav-pills">
  <li><%= link_to "edit", edit_user_profile_path(@user) %></li>
  <li><%= link_to "destroy", user_profile_path(@user), :method => :delete %></li>
</ul>

<% end %>